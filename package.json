{
  "name": "electron-ts-template",
  "version": "0.0.1",
  "description": "A minimal Electron application written with Typescript",
  "author": {
    "name": "Roy Crippen",
    "email": "roycrippen@gmail.com"
  },
  "repository": "https://github.com/roycrippen/electron-ts-template.git",
  "keywords": [
    "Electron",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "main": "dist-js/main-bundle.js",
  "scripts": {
    "start": "npm-run-all build run-electron",
    "build": "webpack --config webpack.config.js",
    "run-electron": "electron .",
    "build:old": "tsc --pretty",
    "watch:old": "tsc -w",
    "start:old": "npm run build && electron ./dist-js/main/main.js",
    "debug:old": "npm run build && electron --inspect ./dist-js/main/main.js"
  },
  "devDependencies": {
    "electron": "^4.2.4",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "@types/node": "^12.0.8",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.0.2",
    "webpack-cli": "^3.3.4"
  }
}
