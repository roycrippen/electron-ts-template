{
  "name": "election-typescipt-template",
  "productName": "election-typescipt-template",
  "version": "0.0.1",
  "description": "An Election with TypeScipt template",
  "license": "BSD-3-Clause",
  "repository": "todo aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
  "author": {
    "name": "Roy Crippen",
    "email": "roycrippen@gmail.com"
  },
  "main": "dist-js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "lint": "tsc && xo && stylelint 'css/*.css'",
    "test": "npm run lint",
    "start": "tsc && electron .",
    "pack": "tsc && electron-builder --dir",
    "dist": "tsc && electron-builder --linux",
    "release": "np --no-publish && npm run publish-snap",
    "publish-snap": "del dist && tsc && electron-builder --linux && snapcraft push --release=stable dist/*.snap"
  },
  "dependencies": {
    "electron-context-menu": "^0.12.1",
    "electron-debug": "^3.0.0",
    "electron-dl": "^1.14.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^2.0.2",
    "electron-store": "3.0.0",
    "electron-updater": "^4.0.6",
    "electron-util": "^0.11.0",
    "element-ready": "^3.1.0",
    "facebook-locales": "^1.0.628",
    "is-online": "^8.2.0",
    "lodash": "^4.17.11",
    "p-wait-for": "^3.1.0"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.1",
    "@types/lodash": "^4.14.132",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "del-cli": "^1.1.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "eslint-config-xo-typescript": "^0.12.0",
    "husky": "^2.3.0",
    "np": "^5.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-xo": "^0.15.0",
    "typescript": "^3.4.5",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "valid-jsdoc": "off",
      "@typescript-eslint/no-object-literal-type-assertion": "off"
    },
    "prettier": true
  },
  "stylelint": {
    "extends": "stylelint-config-xo",
    "rules": {
      "declaration-no-important": null,
      "selector-class-pattern": null,
      "selector-id-pattern": null,
      "no-duplicate-selectors": null,
      "rule-empty-line-before": null
    }
  },
  "prettier": {
    "printWidth": 100,
    "useTabs": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "never",
    "endOfLine": "lf"
  },
  "build": {
    "files": [
      "**/*",
      "!media${/*}"
    ],
    "appId": "election-typescipt-template",
    "linux": {
      "target": [
        "AppImage"
      ],
      "synopsis": "todo aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
      "description": "todo aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
      "category": "Electron; TypeScript"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}